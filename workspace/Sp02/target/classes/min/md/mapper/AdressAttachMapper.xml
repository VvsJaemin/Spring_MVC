<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="min.md.mapper.AddressAttachMapper">


	<insert id="insert">

		insert into address_file (seqf, ofname, sfname, fsize, seq)
		values(#{seqf}, #{ofname}, #{sfname}, #{fsize}, #{seq})

	</insert>

	<delete id="delete">

		delete from address_file where seqf =#{seqf}

	</delete>

	<select id="findBySeq" resultType="AddressAttachVO">

		select*from address_file where seq =#{seq}

	</select>

</mapper>